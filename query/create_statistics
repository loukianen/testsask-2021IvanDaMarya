CREATE TABLE IF NOT EXISTS `statistics` (
  `id` INTEGER PRIMARY KEY AUTOINCREMENT,
  `room` int(11) NOT NULL,
  `created` NUMERIC NOT NULL DEFAULT CURRENT_TIMESTAMP,
  `created_by` int(11) NOT NULL,
  `assigned` NUMERIC NOT NULL,
  `assigned_by` int(11) NOT NULL,
  `modified` NUMERIC NOT NULL,
  `modified_by` int(11) NOT NULL,
  `start` NUMERIC DEFAULT NULL,
  `end` NUMERIC DEFAULT NULL,
  `work` int(11) NOT NULL,
  `staff` int(11) NOT NULL DEFAULT '0',
  `status` tinyint(1) NOT NULL DEFAULT '0',
  `mark` tinyint(1) DEFAULT NULL,
  `bed` tinyint(1) NOT NULL DEFAULT '0',
  `towels` tinyint(1) NOT NULL DEFAULT '0',
  `since` NUMERIC DEFAULT NULL,
  `till` NUMERIC DEFAULT NULL,
  `beds` varchar(50) NOT NULL,
  `return` tinyint(1) NOT NULL,
  `score` float NOT NULL,
  `approved` int(11) NOT NULL
)